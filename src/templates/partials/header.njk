<header class="header">
  <a href="index.html" class="logo">
    <img src="images\Logos.png" alt="Binabox Logo" class="logo__image"/>
  </a>
  <nav class="main-nav">
    <ul class="main-nav__list">
      {% for item in nav %}
      <li>
        <a href="{{ item.url }}" class="main-nav__link {% if item.url == path %}main-nav__link--active{% endif %}">
          {{ item.text }}</a>
      </li>
      {% endfor %}
    </ul>
    <input type="search" placeholder="Search..."/>
    <a class="button button--secondary header__btn" href="#" target="_blank" rel="noopener noreferrer">Discord</a>
    <a class="button button--primary header__btn" href="#" target="_blank" rel="noopener noreferrer">Connect</a>
  </nav>
</header>